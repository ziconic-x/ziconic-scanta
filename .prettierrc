{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "semi": false,
  "printWidth": 110,
  "arrowParens": "always",
  "endOfLine": "auto",
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "useTabs": false,
  "importOrder": [
    "^server-only$",
    "(.*)/__mocks__/(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@/lib/(.*)$",
    "^@/components/(.*)$",
    "^@/components/ui-custom/(.*)$",
    "^@/constants/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
